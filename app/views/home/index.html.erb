<%= Post.find_each do |post| %>
  <% if post.x_offset && post.y_offset && post.width && post.height %>
    <%= image_tag post.photo.variant(resize_to_limit: Post::RESIZE_DIMENSIONS, crop: [post.x_offset, post.y_offset, post.width, post.height]) %>
    <% else %>
      <%= image_tag post.photo.variant(resize_to_limit: Post::RESIZE_DIMENSIONS) %>
  <% end %>
<% end %>